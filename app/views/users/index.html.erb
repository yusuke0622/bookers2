  
 <div class="content-title">
     <h2>User info</h2>
 </div>
 
 <div>
     <%= image_tag @user.get_profile_image(100,100) %>
     <table>
         <tbody>
             <tr>
                 <th>name</th>
                 <th><%= @user.name %></th>
             </tr>
             <tr>
                 <th>introduction</th>
                 <th></th>
             </tr>
         </tbody>
     </table>
     <%= link_to 'config', edit_user_path(@user.id) %>
 </div>
 
 
 
 
 
 <div class="content-title">
     <h2>Users</h2>
 </div>
 
 
<div>
    <div>
        <div>
            <table>
                <thead>
                    <tr>
                        <th>image</th>
                        <th>name</th>
                        <th></th>
                    </tr>
                </thead>
        
                <% @users.each do |user| %>
                <tbody>
                <tr>
                    <td><%= image_tag user.get_profile_image(80,80) %></td>
                    <td><%= user.name %></td>
                    <td><%= link_to 'Show', user_path(user.id) %></td>
                </tr>
                </tbody>
                <% end %>
               
            </table>
        </div>
    </div>
</div>